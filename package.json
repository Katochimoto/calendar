{
  "name": "electron-sample",
  "version": "0.1.0",
  "main": "dist/main.js",
  "author": "rikishi <rikishi.protonmail.ch>",
  "license": "MIT",
  "electronVersion": "1.6.10",
  "scripts": {
    "clean": "rm -rf dist",
    "vendor": "rollup --config rollup.config.vendor.js",
    "dev": "rollup --watch --config rollup.config.js",
    "reload": "livereload 'dist'",
    "watch": "npm-run-all --parallel reload dev vendor",
    "start": "electron .",
    "build": "electron-packager . $npm_package_name --platform=darwin --arch=x64 --version=$npm_package_electronVersion --app-version=$npm_package_version --out=build --ignore=build --prune",
    "test": "mocha --require babel-register src/**/*.spec.js",
    "test-dev": "mocha -w --require babel-register src/**/*.spec.js",
    "flow": "flow check"
  },
  "devDependencies": {
    "autoprefixer": "7.1.1",
    "babel-jest": "20.0.3",
    "babel-plugin-external-helpers": "6.22.0",
    "babel-plugin-module-resolver": "2.7.1",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-flow-strip-types": "6.22.0",
    "babel-plugin-transform-function-bind": "6.22.0",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
    "babel-plugin-transform-react-jsx": "6.24.1",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-es2015-rollup": "3.0.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "buble": "0.15.2",
    "chai": "4.0.0",
    "css-mqpacker": "6.0.0",
    "electron-packager": "^8.7.1",
    "jest": "20.0.4",
    "less-plugin-css-modules": "0.0.3",
    "livereload": "0.6.2",
    "mocha": "3.4.2",
    "npm-run-all": "4.0.2",
    "postcss": "6.0.1",
    "postcss-csso": "2.0.0",
    "rollup": "0.41.6",
    "rollup-plugin-babel": "2.7.1",
    "rollup-plugin-buble": "0.15.0",
    "rollup-plugin-commonjs": "8.0.2",
    "rollup-plugin-filesize": "1.3.2",
    "rollup-plugin-handlebars-plus": "^0.2.0",
    "rollup-plugin-inject": "2.0.0",
    "rollup-plugin-json": "2.1.1",
    "rollup-plugin-less2": "2.0.1",
    "rollup-plugin-node-resolve": "3.0.0",
    "rollup-plugin-preprocess": "0.0.1",
    "rollup-plugin-pug": "^0.1.4",
    "rollup-plugin-replace": "1.1.1",
    "rollup-plugin-uglify": "2.0.1",
    "rollup-watch": "3.2.2"
  },
  "dependencies": {
    "classnames": "2.2.5",
    "es6-symbol": "3.1.1",
    "preact": "8.1.0",
    "preact-compat": "3.16.0",
    "prop-types": "^15.5.10",
    "raf": "3.3.2",
    "setimmediate2": "3.0.0",
    "electron": "1.6.10"
  }
}

{
  "name": "electron-sample",
  "version": "0.0.1",
  "main": "index.js",
  "author": "rikishi <rikishi.protonmail.ch>",
  "license": "MIT",
  "electronVersion": "1.3.4",
  "scripts": {
    "clean": "rm -rf dist",
    "vendor": "rollup --config rollup.config.vendor.js",
    "dev": "rollup --watch --config rollup.config.js",
    "reload": "livereload 'dist'",
    "watch": "npm-run-all --parallel reload dev vendor",
    "start": "electron .",
    "build": "electron-packager . $npm_package_name --platform=darwin --arch=x64 --version=$npm_package_electronVersion --app-version=$npm_package_version --out=build --ignore=build --prune",
    "test": "mocha --require babel-register src/**/*.spec.js",
    "test-dev": "mocha -w --require babel-register src/**/*.spec.js",
    "flow": "flow check"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-jest": "^19.0.0",
    "babel-plugin-module-resolver": "^2.4.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "buble": "^0.15.2",
    "chai": "^3.5.0",
    "css-mqpacker": "^5.0.1",
    "jest": "^19.0.2",
    "less-plugin-css-modules": "^0.0.3",
    "livereload": "^0.6.0",
    "mocha": "^3.2.0",
    "npm-run-all": "^4.0.1",
    "postcss": "^5.2.11",
    "postcss-csso": "^2.0.0",
    "rollup": "^0.41.6",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-filesize": "^1.2.1",
    "rollup-plugin-inject": "^2.0.0",
    "rollup-plugin-json": "^2.1.0",
    "rollup-plugin-less2": "^2.0.1",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-preprocess": "0.0.1",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-plugin-uglify": "^1.0.1",
    "rollup-watch": "^3.2.2"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "preact": "^7.2.0",
    "preact-compat": "^3.14.2",
    "raf": "^3.3.0",
    "setimmediate2": "^3.0.0"
  }
}
